<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="500dp"
    tools:context=".BlankFragment_Date">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="193dp"
        android:layout_marginTop="50dp"
        android:background="@drawable/background_in_heart"
        android:orientation="horizontal">

        <ImageView
            android:id="@+id/img_heat_1"
            android:layout_width="80dp"
            android:layout_height="80dp"
            android:layout_margin="10dp"
            android:background="@drawable/heart__date"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageView
            android:id="@+id/img_heat_2"
            android:layout_width="80dp"
            android:layout_height="80dp"
            android:layout_margin="10dp"
            android:layout_marginEnd="20dp"
            android:background="@drawable/heart__date"
            app:layout_constraintEnd_toStartOf="@+id/img_heat_3"
            app:layout_constraintStart_toEndOf="@id/img_heat_1"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageView
            android:id="@+id/img_heat_3"
            android:layout_width="80dp"
            android:layout_height="80dp"
            android:layout_margin="10dp"
            android:layout_marginStart="28dp"
            android:layout_marginEnd="37dp"
            android:background="@drawable/heart__date"
            app:layout_constraintEnd_toStartOf="@+id/img_heat_4"
            app:layout_constraintStart_toEndOf="@id/img_heat_2"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageView
            android:id="@+id/img_heat_4"
            android:layout_width="80dp"
            android:layout_height="80dp"
            android:layout_margin="10dp"
            android:layout_marginStart="44dp"
            android:background="@drawable/heart__date"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@id/img_heat_3"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/textView2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp"
            android:fontFamily="@font/love_light"
            android:text="Năm"
            android:textSize="25dp"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="@+id/img_heat_1"
            app:layout_constraintStart_toStartOf="@+id/img_heat_1"
            app:layout_constraintTop_toBottomOf="@+id/img_heat_1" />

        <TextView
            android:id="@+id/textView4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp"
            android:fontFamily="@font/love_light"
            android:text="Tháng"
            android:textSize="25dp"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="@+id/img_heat_2"
            app:layout_constraintHorizontal_bias="0.363"
            app:layout_constraintStart_toStartOf="@+id/img_heat_2"
            app:layout_constraintTop_toBottomOf="@+id/img_heat_2" />

        <TextView
            android:id="@+id/textView7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp"
            android:fontFamily="@font/love_light"
            android:text="Tuần"
            android:textSize="25dp"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="@+id/img_heat_3"
            app:layout_constraintStart_toStartOf="@+id/img_heat_3"
            app:layout_constraintTop_toBottomOf="@+id/img_heat_3" />

        <TextView
            android:id="@+id/textView8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp"
            android:fontFamily="@font/love_light"
            android:text="Ngày"
            android:textSize="25dp"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="@+id/img_heat_4"
            app:layout_constraintStart_toStartOf="@+id/img_heat_4"
            app:layout_constraintTop_toBottomOf="@+id/img_heat_4" />

        <TextView
            android:id="@+id/txt_date_in_heart_year"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/kitto"
            android:text="1"
            android:textSize="30dp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="@+id/img_heat_1"
            app:layout_constraintEnd_toEndOf="@+id/img_heat_1"
            app:layout_constraintStart_toStartOf="@+id/img_heat_1"
            app:layout_constraintTop_toTopOf="@+id/img_heat_1" />

        <TextView
            android:id="@+id/txt_date_in_heart_month"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/kitto"
            android:text="1"
            android:textSize="30dp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="@+id/img_heat_2"
            app:layout_constraintEnd_toEndOf="@+id/img_heat_2"
            app:layout_constraintStart_toStartOf="@+id/img_heat_2"
            app:layout_constraintTop_toTopOf="@+id/img_heat_2" />

        <TextView
            android:id="@+id/txt_date_in_heart_week"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/kitto"
            android:text="1"
            android:textSize="30dp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="@+id/img_heat_3"
            app:layout_constraintEnd_toEndOf="@+id/img_heat_3"
            app:layout_constraintStart_toStartOf="@+id/img_heat_3"
            app:layout_constraintTop_toTopOf="@+id/img_heat_3" />

        <TextView
            android:id="@+id/txt_date_in_heart_day"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/kitto"
            android:text="1"
            android:textSize="30dp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="@+id/img_heat_4"
            app:layout_constraintEnd_toEndOf="@+id/img_heat_4"
            app:layout_constraintStart_toStartOf="@+id/img_heat_4"
            app:layout_constraintTop_toTopOf="@+id/img_heat_4" />

        <ImageView
            android:id="@+id/txt_in_heart_hour"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_marginEnd="10dp"
            android:layout_marginBottom="10dp"
            android:background="@drawable/heart__date"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent" />

        <ImageView
            android:id="@+id/txt_in_heart_minimus"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_marginRight="10dp"
            android:layout_marginBottom="10dp"
            android:background="@drawable/heart__date"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintEnd_toStartOf="@id/txt_in_heart_hour" />

        <ImageView
            android:id="@+id/txt_in_heart_sec"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_marginRight="10dp"
            android:layout_marginBottom="10dp"
            android:background="@drawable/heart__date"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintEnd_toStartOf="@id/txt_in_heart_minimus" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="24"
            app:layout_constraintBottom_toBottomOf="@+id/txt_in_heart_sec"
            app:layout_constraintEnd_toEndOf="@+id/txt_in_heart_sec"
            app:layout_constraintStart_toStartOf="@+id/txt_in_heart_sec"
            app:layout_constraintTop_toTopOf="@+id/txt_in_heart_sec" />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="24"
            app:layout_constraintBottom_toBottomOf="@+id/txt_in_heart_minimus"
            app:layout_constraintEnd_toEndOf="@+id/txt_in_heart_minimus"
            app:layout_constraintStart_toStartOf="@+id/txt_in_heart_minimus"
            app:layout_constraintTop_toTopOf="@+id/txt_in_heart_minimus" />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="24"
            app:layout_constraintBottom_toBottomOf="@+id/txt_in_heart_hour"
            app:layout_constraintEnd_toEndOf="@+id/txt_in_heart_hour"
            app:layout_constraintStart_toStartOf="@+id/txt_in_heart_hour"
            app:layout_constraintTop_toTopOf="@+id/txt_in_heart_hour" />
    </androidx.constraintlayout.widget.ConstraintLayout>
</FrameLayout>